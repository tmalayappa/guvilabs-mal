import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        int size = 5; 
        for (int i = 1; i <= size; i++) {
	        for (int j = 1; j <= size; j++) {
	        int value = Math.max(size - j +1, size - i +1 ); 
	        System.out.print(value + " ");		           
	        }
	            
	    System.out.println(); 
	    }
	 }
    
}
